{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
			"linebreak-style": 0,
			"react/jsx-filename-extension": [1, {
				"extensions": [".js", ".jsx"] }
			],
			"react/react-in-jsx-scope": "off",
			"indent": "off",
			"react/jsx-indent": "off",
			"no-tabs": "off",
			"import/prefer-default-export": "off",
			"react/jsx-indent-props": "off",
			"react/jsx-no-bind":0
    }

}
